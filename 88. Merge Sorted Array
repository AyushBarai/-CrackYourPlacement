class Solution {
public:
    void merge(vector<int>& nums1, int m, vector<int>& nums2, int n) {
        int i=0, j=0;
        while(j<n){
            if(j>=n){
                break;
            }
            if(i>=m && j<n){
                nums1.insert(nums1.begin()+i, nums2[j]);
                j++;
            }
            if(nums1[i]>=nums2[j]){
                nums1.insert(nums1.begin()+i, nums2[j]);
                i++;
                j++;
            }else if(nums1[i]<nums2[j]){
                i++;
            }
        }
    }
};
